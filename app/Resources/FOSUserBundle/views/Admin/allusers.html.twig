<h1>get all users</h1>


{% for user in users %}
        <tr>
            <td>{{ user.id }}</td>
            <td>{{ user.name }}</td>
            <td>{{ user.email }}</td>
            <td><a href="users/edit/{{ user.id }}">Edit</a></td>
            <td><a href="users/delete/{{ user.id }}">Delete</a></td>
        </tr>
{% endfor %}