<!DOCTYPE html>
<html lang="lt">
<head>
    {% block head %}
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="description" content="">
        <meta name="author" content="">
        <title>{% block title %}{% endblock %}</title>
        {% block stylesheets %}
            <link href="{{ asset('css/admin/bootstrap.min.css') }}"
                  rel="stylesheet">
            <!--[if lt IE 9]>
            <script src="{{ asset('js/html5shiv.min.js') }}"></script>
            <script src="{{ asset('js/respond.min.js') }}"></script>
            <![endif]-->
        {% endblock %}
    {% endblock %}
</head>

<body>
{% block body %}
    {% block page_wrapper %}
    {% endblock %}
{% endblock %}

{% block javascripts %}
    <script src="{{ asset('js/jquery.min.js') }}"></script>
    <script src="{{ asset('js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('js/admin/sweetalert.min.js') }}"></script>
    <script src="{{ asset('js/admin/metisMenu.min.js') }}"></script>
    <script src="{{ asset('js/admin/sb-admin-2.js') }}"></script>
    <script type="text/javascript">
        var translations = {
            'title': '{{ 'sweetAlert_title' | trans }}',
            'confirmButtonText': '{{ 'sweetAlert_confirmButtonText' | trans }}',
            'cancelButtonText': '{{ 'sweetAlert_cancelButtonText' | trans }}',
            'deleted': '{{ 'sweetAlert_deleted' | trans }}',
            'deletedText': '{{ 'sweetAlert_deletedText' | trans }}'
        }
    </script>
    <script src="{{ asset('js/admin/masterpeace.js') }}"></script>
{% endblock %}
</body>
</html>